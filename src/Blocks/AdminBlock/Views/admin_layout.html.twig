<!DOCTYPE html>
<html lang="fr">
    <head>
        <title>{{ app_name }} Admin</title>

        <!-- BEGIN META -->
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="keywords" content="your,keywords">
        <meta name="description" content="{{ app_name }} admin panel">
        <meta name="_token" content="{{ token() }}">
        <!-- END META -->

        <link rel="icon" href="{{ asset('upload/default/favicon.ico') }}">

        <!-- BEGIN STYLESHEETS -->
        {#<link rel="stylesheet" href="{{ assetic_stylesheets('admin_css', 'yui_css', {'output':'/admin.css'}) }}">#}
        {% stylesheets 'admin_css' filter='yui_css' output='/asset/dist/admin.css' %}
            <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
        {% endstylesheets %}
        {% block css %}{% endblock %}
        <!-- END STYLESHEETS -->

        <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->

    </head>

    <body>

        {% include('Include/sidebar.html.twig') %}

        {% include('Include/header.html.twig') %}

        <!-- Main container -->
        <main>
            <div class="main-content">
                {% block body %}{% endblock %}
            </div><!--/.main-content -->

            {% include('Include/footer.html.twig') %}

        </main>

        <!-- BEGIN JAVASCRIPT -->
        {#{{ assets(libs, 'js') }}#}
        {% javascripts 'admin_js' filter='yui_js' output='/asset/dist/admin.js'%}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% block js %}{% endblock %}
        <!-- END JAVASCRIPT -->

    </body>
</html>