{
  "name": "{{APP_NAME}}",
  "domain": "{{DOMAIN}}",
  "admin_domain": "{{ADMIN_DOMAIN}}",
  "sub_domain": {{SUB_DOMAIN}},

  "environment": "{{ENV}}",
  "maintenance": {{MAINTENANCE}},

  "db": {
    "default": {
      "driver": "{{DB_DRIVER}}",
      "host": "{{DB_HOST}}",
      "user": "{{DB_USER}}",
      "pass": "{{DB_PASS}}",
      "db": "{{DB_NAME}}",
      "prefix": "{{DB_PREFIX}}"
