      "user": "{{MAIL_USER}}",
      "pass": "{{MAIL_PASS}}",
      "port": {{MAIL_PORT}},
      "charset": "UTF-8"
    },
    "from": {
      "{{MAIL_CONTACT}}": "Contact {{APP_NAME}}"
    }
  },

  "params": {

      "phone": "01 82 83 72 30",
      "email": "support@web-zy.com",

      "address": {
        "road": "85 Avenue Henri Barbusse",
        "postal_code": "92140",
        "city": "Clamart",
        "country": "FRANCE"
      },

      "social": {
        "facebook": "#",
        "twitter": "#",
        "instagram": "#"
      }

    },


  "payment": {
      "stripe": {
        "public_key": "{{STRIPE_PUBLIC_KEY}}",
        "secret_key": "{{STRIPE_SECRET_KEY}}",
        "currency": "eur",
        "tva": 20
