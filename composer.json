{
    "name": "jetfirephp/jetbuilder",
    "type": "cms",
    "description": "JetBuilder - CMS",
    "keywords": ["CMS","Buider"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sumugan Sinnarasa",
            "email": "mugan27@gmail.com",
            "role": "Developer"
        }
    ],
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require-dev": {
        "symfony/yaml": "v2.8.2",
        "codeception/codeception": "^2.2"
    },
    "require": {
        "php": ">=5.4.0",
        "jetfirephp/framework": "@dev",
        "doctrine/orm": "^2.5",
        "doctrine/data-fixtures": "~1.1",
        "beberlei/DoctrineExtensions": "^1.0",
        "twig/twig": "^2.3",
        "phpmailer/phpmailer": "~5.2",
        "tinify/tinify": "^1.3",
        "knplabs/github-api": "^1.7",
        "php-http/guzzle6-adapter": "^1.1",
        "symfony/filesystem": "^3.1",
        "doctrine/migrations": "^1.5",
        "cocur/slugify": "^2.3",
        "stripe/stripe-php": "^4.9",
        "mikehaertl/phpwkhtmltopdf": "^2.2",
        "phive/twig-extensions-deferred": "2.0",
        "symfony/translation": "v3.2.*"
    },
    "autoload": {
        "psr-4": {
            "Jet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jet\\Test\\": "tests/phpunit/src/"
        }
    },
    "minimum-stability": "dev"
}
